                      function getAssetURL(){
                        var url = window.location.href;
                        return url;
                      }
  
                      function getReturnURL(){
                        var url = window.location.href;
  
                            if ( !(url.indexOf('?' + 'success=true') != -1) || (url.indexOf('&' + 'success=true') != -1) ) {
                              if (url.indexOf('?') != -1)
                              {
                                url=url+"&success=true";
                              }
                              else {
                                url=url+"?success=true";
                              }
                            }
                        return url;
                      }
  
                      function hsRedirectToAsset() {
                        
                        
                        var clnURL = hsClearURL();
                        
                        Cookies.set("gated", clnURL , {expires: 7});
                        
                        
                        var docUrl = getReturnURL();
                        
                        setTimeout(function () {
                              window.top.location = docUrl;
                            }, 1000);
                        
                      };
                    
                      
                    
                    function hsClearURL() {
                      return [location.protocol, '//', location.host, location.pathname].join('');
                    }
                
